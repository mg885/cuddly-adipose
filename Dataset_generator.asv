%generate synthetic images and save them in .mat files

%%
%settings
Grade = 0; % image grade, use interval 0-4
num_of_images = 1; % number of images
mkdir('Dataset');

for k=1:num_of_images
    imgdata = Image_gen(Grade);
    name = ['Dataset/igrade_' num2str(Grade) num2str(k) '.mat'];
    save(name, 'imgdata');
end
